	<servlet-mapping>
		<servlet-name>UpdateParameterServlet</servlet-name>
		<url-pattern>/update_system_parameter.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddLeaveEntitlementServlet</servlet-name>
		<url-pattern>/add_leave_entitlement.do</url-pattern>
