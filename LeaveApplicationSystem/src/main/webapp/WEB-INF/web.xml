		<display-name>ReportsServlet</display-name>
		<description></description>
		<servlet-class>com.pactera.servlet.ReportsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProfileServlet</servlet-name>
		<url-pattern>/profile.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LeaveEntitlementServlet</servlet-name>
		<url-pattern>/leaveentitlement.do</url-pattern>
	</servlet-mapping>
