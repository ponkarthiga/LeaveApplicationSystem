		<servlet-class>com.pactera.servlet.AddLeaveEntitlementServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddSystemParameterServlet</servlet-name>
		<display-name>AddSystemParameterServlet</display-name>
		<description></description>
		<servlet-class>com.pactera.servlet.AddSystemParameterServlet</servlet-class>
	</servlet>
