<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ProfileServlet</servlet-name>
		<servlet-class>com.pactera.servlet.ProfileServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.pactera.servlet.LoginServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>LeaveServlet</servlet-name>
		<display-name>LeaveServlet</display-name>
		<description></description>
		<servlet-class>com.pactera.servlet.LeaveServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateLeaveServlet</servlet-name>
		<display-name>UpdateLeaveServlet</display-name>
		<description></description>
		<servlet-class>com.pactera.servlet.UpdateLeaveServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ShowLeaveServlet</servlet-name>
		<display-name>ShowLeaveServlet</display-name>
		<description></description>
		<servlet-class>com.pactera.servlet.ShowLeaveServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LeaveListServlet</servlet-name>
		<display-name>LeaveListServlet</display-name>
		<description></description>
		<servlet-class>com.pactera.servlet.LeaveListServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteLeaveServlet</servlet-name>
		<display-name>DeleteLeaveServlet</display-name>
		<description></description>
		<servlet-class>com.pactera.servlet.DeleteLeaveServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProfileServlet</servlet-name>
		<url-pattern>/profile.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>LeaveServlet</servlet-name>
		<url-pattern>/leave.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateLeaveServlet</servlet-name>
		<url-pattern>/updleave.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ShowLeaveServlet</servlet-name>
		<url-pattern>/showleave.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LeaveListServlet</servlet-name>
		<url-pattern>/leavelist.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteLeaveServlet</servlet-name>
		<url-pattern>/delleave.do</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
</web-app>
