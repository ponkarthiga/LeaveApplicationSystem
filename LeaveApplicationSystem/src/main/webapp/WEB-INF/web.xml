	<servlet>
		<servlet-name>UpdateLeaveEntitlementServlet</servlet-name>
		<display-name>UpdateLeaveEntitlementServlet</display-name>
		<description></description>
		<servlet-class>com.pactera.servlet.UpdateLeaveEntitlementServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SystemParameterServlet</servlet-name>
		<display-name>SystemParameterServlet</display-name>
